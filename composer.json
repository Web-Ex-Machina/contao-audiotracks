{
    "name": "webexmachina/contao-audiotracks",
    "description": "Contao Audiotracks bundle for Contao Open Source CMS",
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "website" : "https://www.webexmachina.fr/",
    "authors": [
        {
            "name": "Web ex Machina",
            "homepage": "https://www.webexmachina.fr",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/webexmachina/contao-audiotracks/issues",
        "source":"https://github.com/webexmachina/contao-audiotracks"
    },
    "require": {
        "php": "^7.4|^8.0",
        "contao/core-bundle": "^4.9",
        "webexmachina/contao-utils": "^0.3"
    },
    "autoload":{
        "psr-4": {
            "WEM\\AudioTracksBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra":{
        "contao-manager-plugin": "WEM\\AudioTracksBundle\\ContaoManager\\Plugin"
    }
}